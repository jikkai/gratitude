<% extend('../layout/layout.art') %>
{{block 'title'}}{{title}} - Links{{/block}}

{{block 'content'}}
<div class="site-links">
  {{each page.links}}
  <section class="site-links__block">
    <h3>
      <a href="{{$value.link}}" target="_blank">
        {{$value.title}}
      </a>
    </h3>
    <span>{{$value.author}}</span>
    <p>{{$value.desc}}</p>
  </section>
  {{/each}}
</div>
{{/block}}
